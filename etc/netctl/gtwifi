Description='A wireless connection using a custom network block configuration'
Interface=wlp2s0
Connection=wireless
Security=wpa-configsection
IP=dhcp
WPAConfigSection=(
    'ssid="GTwifi"'
    'proto=WPA2'
    'key_mgmt=WPA-EAP'
    'eap=PEAP'
    'identity="cknight7"'
    'password="whrpl#!FWfw"'
    'ca_cert="/etc/ssl/certs/AddTrust_External_Root.pem"'
    'priority=1'
    'phase2="auth=MSCHAPV2"'
)
